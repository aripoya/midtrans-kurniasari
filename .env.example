# Environment Variables for Production

# Midtrans Configuration
MIDTRANS_SERVER_KEY=your_production_server_key_here
MIDTRANS_CLIENT_KEY=your_production_client_key_here
MIDTRANS_IS_PRODUCTION=true

# Application Configuration
APP_NAME=Order Management System
APP_URL=https://your-domain.com
API_URL=https://your-worker.your-subdomain.workers.dev

# Database Configuration (Cloudflare D1)
DATABASE_NAME=order_management_prod

# Security
WEBHOOK_SECRET=your_webhook_secret_here

# Optional: Email Configuration
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USER=your-email@gmail.com
SMTP_PASS=your-app-password

